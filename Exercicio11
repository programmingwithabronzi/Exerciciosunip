using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace ConsoleApplication1
{
    class Program
    {
        static void Main(string[] args)
        {

            Console.WriteLine("Programa que troca os valores");

            //Declaração de variáveis

            int a, b, c;

            //Entrada de dados

            Console.Write("\nDigite um número inteiro para o A: ");
            a = int.Parse(Console.ReadLine());

            Console.Write("\nDigite um outro inteiro para o B: ");
            b = int.Parse(Console.ReadLine());
        
            //Processamento

            c = a;
        
            a = b;

            b = c;
        
            //Saída

            Console.Write("\nOs valores de A é {0} e o valor de B é {1}", a, b);

            Console.ReadKey();
        }
    }
}
